{
  "name": "flux-session-monitor",
  "version": "1.0.0",
  "description": "A simple Electron app to monitor web session data.",
  "main": "monitor-main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "lint": "eslint ."
  },
  "build": {
    "appId": "com.flux.autodeleter",
    "productName": "Flux Auto Deleter",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "dir"
      ],
      "sign": null
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Gemini",
  "license": "ISC",
  "devDependencies": {
    "electron": "^39.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.57.1"
  },
  "dependencies": {
    "ini": "^4.1.2",
    "node-fetch": "^2.7.0"
  }
}
